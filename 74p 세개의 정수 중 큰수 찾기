#include <iostream>
using namespace std;
int main()
{
	//세 개의 정수 중에서 가장 큰수 찾는 프로그램
	
	//세개의 정수
	int num1, num2, num3;
	//가장 큰 정수
	int large_num;
	//3개의 정수 입력
	cout << "3개의 정수를 입력하세요 :";
	cin >> num1 >> num2 >> num3;
	//큰 정수 계산
	if (num1 > num2 && num1 > num3)
		large_num = num1;
	if (num2 > num1 && num2 > num3)
		large_num = num2;
	else //num3이 클경우
		large_num = num3;
	//큰 정수 출력
	cout << "가장 큰 정수는 " << large_num <<endl;



}
;
